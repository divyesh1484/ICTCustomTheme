<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="IctGeneralInfo\Core\Content\IctGeneralInfo\IctGeneralInfoDefinition">
            <tag name="shopware.entity.definition" entity="ict_general_info"/>
        </service>

        <service id="IctGeneralInfo\Core\Content\IctGeneralInfo\Aggregate\IctGeneralInfoTranslation\IctGeneralInfoTranslationDefinition">
            <tag name="shopware.entity.definition" entity="ict_general_info_translation"/>
        </service>

        <service id="IctGeneralInfo\Core\Content\Extension\MediaExtension">
            <tag name="shopware.entity.extension"/>
        </service>

        <service id="IctGeneralInfo\Core\Content\Extension\LanguageExtension">
            <tag name="shopware.entity.extension"/>
        </service>
        
        <service id="IctGeneralInfo\Core\Content\Extension\CountryExtension">
            <tag name="shopware.entity.extension"/>
        </service>

        <service id="IctGeneralInfo\Core\Content\Extension\CountryStateExtension">
            <tag name="shopware.entity.extension"/>
        </service>

        <service id="IctGeneralInfo\Core\Content\Extension\ProductExtension">
            <tag name="shopware.entity.extension"/>
        </service>
    </services>
</container>